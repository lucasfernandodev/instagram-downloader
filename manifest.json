{
  "name": "Instagram video downloader",
  "version": "1.0",
  "description": "Extens√£o para baixar videos do instagram",
  "icons": {
    "128": "./assets/img/icon.png"
  },
  "background": {
    "service_worker": "./src/sw.js",
    "type": "module"
  },
  "homepage_url": "https://github.com/lucasfernandodev/instagram-downloader",
  "host_permissions": ["*://*.instagram.com/*"],
  "permissions": ["storage",
  "contextMenus",
  "tabs",
  "activeTab",
   "scripting",
   "webNavigation",
   "history"
  ],
  "action": {
    "default_popup": "./popup.html"
  },
  "web_accessible_resources": [{
    "resources": ["src/downloadFile.js"],
    "matches": ["<all_urls>"]
  }],
  "manifest_version": 3
}